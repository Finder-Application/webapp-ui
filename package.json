{
  "name": "webapp-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "format": "prettier --write ./src",
    "prettier:fix": "prettier --write",
    "prettier:check": "prettier --check ."
  },
  "dependencies": {
    "antd": "^4.22.8",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.8",
    "sass": "^1.54.8"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "husky": "7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn prettier:fix"
    ]
  }
